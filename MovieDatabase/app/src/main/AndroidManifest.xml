<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mahi.homework2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddActivity"
            android:label="@string/add_movie_activity"/>
        <activity android:name=".EditActivity"
            android:label="@string/edit_movie_activity"/>
        <activity android:name=".ShowbyyearActivity"
            android:label="@string/listbyyear_activity"/>
        <activity android:name=".ShowbyratingActivity"
            android:label="@string/listbyrating_activity"></activity>
    </application>

</manifest>